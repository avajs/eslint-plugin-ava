{
	"name": "eslint-plugin-ava",
	"version": "10.5.0",
	"description": "ESLint rules for AVA",
	"license": "MIT",
	"repository": "avajs/eslint-plugin-ava",
	"engines": {
		"node": ">=10.18.0 <11 || >=12.14.0 <13 || >=14"
	},
	"scripts": {
		"test": "xo && c8 ava",
		"integration": "node ./test/integration/test.js"
	},
	"files": [
		"index.js",
		"create-ava-rule.js",
		"util.js",
		"rules"
	],
	"keywords": [
		"eslint",
		"eslintplugin",
		"eslint-plugin",
		"ava",
		"test",
		"runner",
		"assert",
		"asserts",
		"assertion",
		"mocha"
	],
	"dependencies": {
		"deep-strict-equal": "^0.2.0",
		"enhance-visitors": "^1.0.0",
		"eslint-utils": "^2.1.0",
		"espree": "^7.1.0",
		"espurify": "^2.0.1",
		"import-modules": "^2.0.0",
		"micro-spelling-correcter": "^1.1.1",
		"pkg-dir": "^4.2.0",
		"resolve-from": "^5.0.0"
	},
	"devDependencies": {
		"ava": "^3.11.1",
		"babel-eslint": "^10.1.0",
		"c8": "^7.3.0",
		"chalk": "^4.1.0",
		"del": "^5.1.0",
		"eslint": "6.2.0",
		"eslint-ava-rule-tester": "^4.0.0",
		"eslint-plugin-eslint-plugin": "^2.3.0",
		"execa": "^4.0.3",
		"listr": "^0.14.3",
		"outdent": "^0.7.1",
		"pify": "^5.0.0",
		"tempy": "^0.6.0",
		"xo": "^0.33.0"
	},
	"peerDependencies": {
		"eslint": ">=6.2.0"
	},
	"ava": {
		"files": [
			"!rules",
			"test/*.js"
		]
	},
	"xo": {
		"plugins": [
			"eslint-plugin"
		],
		"extends": [
			"plugin:eslint-plugin/all"
		],
		"overrides": [
			{
				"files": "create-ava-rule.js",
				"rules": {
					"eslint-plugin/require-meta-docs-url": "off"
				}
			}
		],
		"rules": {
			"ava/no-ignored-test-files": "off"
		}
	},
	"c8": {
		"reporter": [
			"html",
			"lcov",
			"text"
		]
	}
}
