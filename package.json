{
	"name": "eslint-plugin-ava",
	"version": "6.0.0",
	"description": "ESLint rules for AVA",
	"license": "MIT",
	"repository": "avajs/eslint-plugin-ava",
	"engines": {
		"node": ">=8"
	},
	"scripts": {
		"test": "xo && nyc ava",
		"integration": "node ./test/integration/test.js"
	},
	"files": [
		"index.js",
		"create-ava-rule.js",
		"util.js",
		"rules"
	],
	"keywords": [
		"eslint",
		"eslintplugin",
		"eslint-plugin",
		"ava",
		"test",
		"runner",
		"assert",
		"asserts",
		"assertion",
		"mocha"
	],
	"dependencies": {
		"arrify": "^2.0.1",
		"deep-strict-equal": "^0.2.0",
		"enhance-visitors": "^1.0.0",
		"esm": "^3.2.25",
		"espree": "^5.0.0",
		"espurify": "^2.0.0",
		"import-modules": "^1.1.0",
		"is-plain-object": "^3.0.0",
		"multimatch": "^4.0.0",
		"pkg-up": "^3.1.0"
	},
	"devDependencies": {
		"ava": "^1.0.1",
		"babel-eslint": "^10.0.1",
		"chalk": "^2.4.2",
		"del": "^4.1.1",
		"eslint": "^5.12.0",
		"eslint-ava-rule-tester": "^3.0.0",
		"execa": "^1.0.0",
		"js-combinatorics": "^0.5.4",
		"listr": "^0.14.3",
		"codecov": "^3.5.0",
		"eslint-plugin-eslint-plugin": "2.1.0",
		"nyc": "^14.1.1",
		"pify": "^4.0.1",
		"tempy": "^0.3.0",
		"xo": "^0.24.0"
	},
	"peerDependencies": {
		"eslint": ">=5.9.0"
	},
	"ava": {
		"files": [
			"test/*js"
    ]
  },
	"xo": {
		"plugins": [
			"eslint-plugin"
		],
		"extends": [
			"plugin:eslint-plugin/all"
		],
		"overrides": [
			{
				"files": "create-ava-rule.js",
				"rules": {
					"eslint-plugin/require-meta-docs-url": "off"
				}
			}
		]
	},
	"nyc": {
		"reporter": [
			"html",
			"lcov",
			"text"
		]
	}
}
